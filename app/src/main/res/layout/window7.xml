<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Window7_Activity"
    android:background="#CF75F8">

    <TextView
    android:id="@+id/W7_Title"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:text="FIN HEALD"
    android:textAlignment="center"
    android:textColor="#000000"
    android:textSize="30sp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="MissingConstraints" />


    <ImageButton
        android:id="@+id/w7_incomesForm2"
        android:layout_width="295dp"
        android:layout_height="406dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/cuenta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/W7_Title" />

    <Button
        android:id="@+id/w7_confirm"
        android:layout_width="291dp"
        android:layout_height="60dp"
        android:layout_marginTop="40dp"
        android:text="CONFIRM"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w7_incomesForm2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/w7_valor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Value"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="@+id/w7_incomesForm2"
        app:layout_constraintTop_toTopOf="@+id/w7_incomesForm2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/w2_valueText"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:textAlignment="center"
        android:hint="$$"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/w7_incomesForm2"
        app:layout_constraintTop_toBottomOf="@+id/w7_valor"
        tools:ignore="MissingConstraints" />


    <EditText
        android:id="@+id/w2_dateText2"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:hint="dd/mm/aa"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/w7_incomesForm2"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/w7_incomesForm2"
        app:layout_constraintTop_toBottomOf="@+id/w7_fecha"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/w7_fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Date"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/w7_incomesForm2"
        app:layout_constraintTop_toBottomOf="@+id/w2_valueText"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/w4_barra"
        android:layout_width="410dp"
        android:layout_height="87dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/w4_home"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintStart_toEndOf="@+id/w4_menu"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.518"
        tools:ignore="MissingConstraints"
        android:onClick="goHome"/>

    <ImageView
        android:id="@+id/w4_menu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="56dp"
        android:src="@drawable/men_"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintStart_toStartOf="@+id/w4_barra"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.518"
        android:onClick="GoMenu"/>

    <ImageView
        android:id="@+id/w4_calenday"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:src="@drawable/calendary"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintEnd_toEndOf="@+id/w4_barra"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toEndOf="@+id/w4_home"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.468" />

</androidx.constraintlayout.widget.ConstraintLayout>