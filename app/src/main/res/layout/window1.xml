<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Window1_Activity">

    <TextView
        android:id="@+id/AppTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="FIN HEALD"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/w1_fondo"
        android:layout_width="412dp"
        android:layout_height="425dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/AppTitle"
        app:srcCompat="@drawable/whatsapp_image_2023_03_25_at_9_31_22_am"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp" />

    <Button
        android:id="@+id/w1_Login"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#8C25CD"
        android:text="LogIn"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w1_fondo"
        tools:ignore="MissingConstraints"
        android:onClick="ClickLoginW1"
        />

    <Button
        android:id="@+id/w1_SignUp"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:backgroundTint="#1DFBFBFB"
        android:text="SignUp"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w1_Login"
        app:layout_constraintVertical_bias="0.0"
        android:onClick="ClickSignW1"/>

</androidx.constraintlayout.widget.ConstraintLayout>