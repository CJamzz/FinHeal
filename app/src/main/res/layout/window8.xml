<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Window8_Activity"
    android:background="#CF75F8">

    <ImageButton
        android:id="@+id/w7_incomesForm2"
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:src="@drawable/cuenta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="-5dp"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/w4_barra"
        android:layout_width="410dp"
        android:layout_height="87dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        />

    <ImageView
        android:id="@+id/w4_home"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintStart_toEndOf="@+id/w4_menu"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.518"
        tools:ignore="MissingConstraints"
        android:onClick="GoHome"/>

    <ImageView
        android:id="@+id/w4_menu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="56dp"
        android:src="@drawable/men_"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintStart_toStartOf="@+id/w4_barra"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.518" />

    <ImageView
        android:id="@+id/w4_calenday"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:src="@drawable/calendary"
        app:layout_constraintBottom_toBottomOf="@+id/w4_barra"
        app:layout_constraintEnd_toEndOf="@+id/w4_barra"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toEndOf="@+id/w4_home"
        app:layout_constraintTop_toTopOf="@+id/w4_barra"
        app:layout_constraintVertical_bias="0.468" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="108dp"
        android:layout_height="102dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/w7_incomesForm2"
        app:layout_constraintStart_toStartOf="@+id/w7_incomesForm2"
        app:layout_constraintTop_toTopOf="@+id/w7_incomesForm2"
        app:layout_constraintVertical_bias="0.6"
        app:srcCompat="@drawable/user" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="40dp"
        android:text="Hello Username"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/w7_incomesForm2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acount 0000000000-01"
        android:textColor="#000000"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/w7_incomesForm2"
        app:layout_constraintEnd_toEndOf="@+id/w7_incomesForm2"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.186" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="67dp"
        android:layout_height="59dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w7_incomesForm2"
        app:srcCompat="@drawable/cuentas" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Acounts"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/w7_incomesForm2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="38dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/logout" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Logout"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/w4_barra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.026" />

</androidx.constraintlayout.widget.ConstraintLayout>